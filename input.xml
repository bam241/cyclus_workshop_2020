<simulation>

    <control>
        <duration>2300</duration>
        <startmonth>1</startmonth>
        <startyear>2000</startyear>
        <decay>never</decay>
        <tolerance_resource>1e-5</tolerance_resource>
        <explicit_inventory>true</explicit_inventory>
    </control>

    <archetypes>
        <spec>
            <lib>cycamore</lib>
            <name>Source</name>
        </spec>
        <spec>
            <lib>cycamore</lib>
            <name>DeployInst</name>
        </spec>
        <spec>
            <lib>cycamore</lib>
            <name>Sink</name>
        </spec>
        <spec>
            <lib>cycamore</lib>
            <name>Enrichment</name>
        </spec>
        <spec>
            <lib>cycamore</lib>
            <name>Reactor</name>
        </spec>
        <spec>
            <lib>cycamore</lib>
            <name>Storage</name>
        </spec>
        <spec>
            <lib>cycamore</lib>
            <name>FuelFab</name>
        </spec>
        <spec>
            <lib>cycamore</lib>
            <name>Separations</name>
        </spec>
        <spec>
            <lib>agents</lib>
            <name>NullRegion</name>
        </spec>
        <spec>
            <lib>agents</lib>
            <name>NullInst</name>
        </spec>
    </archetypes>

    <facility>
        <config>
            <Source>
                <outcommod>c_uore</outcommod>
                <outrecipe>r_nat_u</outrecipe>
                <throughput>10006.469906504619</throughput>
            </Source>
        </config>
        <name>UraniumMine</name>
    </facility>

    <facility>
        <config>
            <Enrichment>
                <feed_commod>c_uore</feed_commod>
                <feed_recipe>r_nat_u</feed_recipe>
                <initial_feed>0</initial_feed>
                <max_enrich>0.041</max_enrich>
                <max_feed_inventory>10000</max_feed_inventory>
                <product_commod>c_leu</product_commod>
                <swu_capacity>100000</swu_capacity>
                <tails_commod>c_u_depleted</tails_commod>
            </Enrichment>
        </config>
        <name>LEUEnrichmentFacility</name>
    </facility>

    <facility>
        <config>
            <Reactor>
                <assem_size>295</assem_size>
                <cycle_time>18</cycle_time>
                <fuel_incommods>
                    <val>c_leu</val>
                </fuel_incommods>
                <fuel_inrecipes>
                    <val>r_leu</val>
                </fuel_inrecipes>
                <fuel_outcommods>
                    <val>c_spent_leu</val>
                </fuel_outcommods>
                <fuel_outrecipes>
                    <val>r_leu_spent</val>
                </fuel_outrecipes>
                <n_assem_batch>1</n_assem_batch>
                <n_assem_core>3</n_assem_core>
                <power_cap>90</power_cap>
                <refuel_time>1</refuel_time>
            </Reactor>
        </config>
        <name>LWR</name>
    </facility>

    <facility>
        <config>
            <Storage>
                <in_commods>
                    <val>c_spent_leu</val>
                </in_commods>
                <out_commods>
                    <val>c_spent_leu_stored</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
        <name>SpentFuelStorage</name>
    </facility>
    
    <facility>
        <config>
            <Storage>
                <in_commods>
                    <val>c_u_depleted</val>
                </in_commods>
                <out_commods>
                    <val>c_u_depleted_stored</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
        <name>DepletedUStorage</name>
    </facility>

    <recipe>
        <basis>mass</basis>
        <name>r_nat_u</name>
        <nuclide>
            <comp>0.00711</comp>
            <id>92235</id>
        </nuclide>
        <nuclide>
            <comp>0.99289</comp>
            <id>92238</id>
        </nuclide>
    </recipe>
    <recipe>
        <basis>mass</basis>
        <name>r_heu</name>
        <nuclide>
            <comp>0.9</comp>
            <id>92235</id>
        </nuclide>
        <nuclide>
            <comp>0.1</comp>
            <id>92238</id>
        </nuclide>
    </recipe>
    <recipe>
        <basis>mass</basis>
        <name>r_leu</name>
        <nuclide>
            <comp>0.04</comp>
            <id>92235</id>
        </nuclide>
        <nuclide>
            <comp>0.96</comp>
            <id>92238</id>
        </nuclide>
    </recipe>
    <recipe>
        <basis>mass</basis>
        <name>r_leu_spent</name>
        <nuclide>
            <comp>0.01</comp>
            <id>92235</id>
        </nuclide>
        <nuclide>
            <comp>0.94</comp>
            <id>92238</id>
        </nuclide>
        <nuclide>
            <comp>0.01</comp>
            <id>94239</id>
        </nuclide>
        <nuclide>
            <comp>0.3</comp>
            <id>55135</id>
        </nuclide>
    </recipe>


    <region>
        <config>
            <NullRegion/>
        </config>
        <institution>
            <config>
                <NullInst/>
            </config>
            <initialfacilitylist>
                <entry>
                    <number>1</number>
                    <prototype>UraniumMine</prototype>
                </entry>
                <entry>
                    <number>1</number>
                    <prototype>LEUEnrichmentFacility</prototype>
                </entry>
                <entry>
                    <number>1</number>
                    <prototype>LWR</prototype>
                </entry>
                <entry>
                    <number>1</number>
                    <prototype>SpentFuelStorage</prototype>
                </entry>
                <entry>
                    <number>1</number>
                    <prototype>DepletedUStorage</prototype>
                </entry>
                
            </initialfacilitylist>
            <name>inst</name>
        </institution>
        <name>MyRegion</name>
    </region>






</simulation>
