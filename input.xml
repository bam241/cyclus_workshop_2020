<simulation>
	<control>
		<duration>200</duration>
		<startmonth>1</startmonth>
		<startyear>0</startyear>
		<decay>never</decay>
		<explicit_inventory>true</explicit_inventory>
	</control>

	<archetypes>
		<spec>
			<lib>cycamore</lib>
			<name>Source</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Sink</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Reactor</name>
		</spec>
		<spec>
			<lib>cycamore</lib>
			<name>Enrichment</name>
		</spec>
		<spec>
			<lib>agents</lib>
			<name>NullRegion</name>
		</spec>
		<spec>
			<lib>agents</lib>
			<name>NullInst</name>
		</spec>
	</archetypes>

	<facility>
		<config>
			<Source>
				<outcommod>c_uore</outcommod>
				<outrecipe>r_nat_u</outrecipe>
				<throughput>10</throughput>
			</Source>
		</config>
		<name>UraniumMine</name>
	</facility>
	
	<facility>
		<config>
			<Enrichment>
				<feed_commod>c_uore</feed_commod>
				<feed_recipe>r_nat_u</feed_recipe>
				<initial_feed>0</initial_feed>
				<max_enrich>0.05</max_enrich>
				<max_feed_inventory>10000</max_feed_inventory>
				<product_commod>c_leu</product_commod>
				<swu_capacity>100000</swu_capacity>
				<tails_commod>c_u_depleted</tails_commod>			
			</Enrichment>
		</config>
		<name>UraniumEnrichement</name>
	</facility>
	
	<facility>
		<config>
			<Reactor>
				<fuel_incommods>
					<val>c_mox</val>
					<val>c_leu</val>
				</fuel_incommods>
				<fuel_inrecipes>
					<val>r_mox</val>
					<val>r_leu</val>
				</fuel_inrecipes>
				<fuel_prefs>
					<val>100</val>
					<val>1</val>
				</fuel_prefs>
				<fuel_outcommods>
					<val>c_mox_irradiated</val>
					<val>c_leu_irradiated</val>
				</fuel_outcommods>
				<fuel_outrecipes>
					<val>r_mox_irratiaded</val>
					<val>r_leu_irratiaded</val>
				</fuel_outrecipes>
				<assem_size>10</assem_size>
				<n_assem_batch>1</n_assem_batch>
				<n_assem_core>3</n_assem_core>
				<cycle_time>12</cycle_time>
				<power_cap>10</power_cap>
			</Reactor>
		</config>
		<name>PWR</name>
	</facility>
	<facility>
		<config>
			<Sink>
				<in_commods>
					<val>c_leu_irradiated</val>
				</in_commods>
			</Sink>
		</config>
		<name>IrradiatedUraniumSink</name>
	</facility>
	<facility>
		<config>
			<Sink>
				<in_commods>
					<val>c_u_depleted</val>
				</in_commods>
			</Sink>
		</config>
		<name>DepletedUraniumSink</name>
	</facility>


	<region>
		<config>
			<NullRegion/>
		</config>
		<institution>
			<config>
				<NullInst/>
			</config>
			<initialfacilitylist>
				<entry>
					<number>1</number>
					<prototype>UraniumMine</prototype>
				</entry>
				<entry>
					<number>1</number>
					<prototype>UraniumEnrichement</prototype>
				</entry>
				<entry>
					<number>1</number>
					<prototype>PWR</prototype>
				</entry>
				<entry>
					<number>1</number>
					<prototype>IrradiatedUraniumSink</prototype>
				</entry>
				<entry>
					<number>1</number>
					<prototype>DepletedUraniumSink</prototype>
				</entry>
			</initialfacilitylist>
			<name>My_Inst</name>
		</institution>
		<name>My_Region</name>
	</region>




	<recipe>
		<basis>mass</basis>
		<name>r_nat_u</name>
		<nuclide>
			<comp>0.00711</comp>
			<id>92235</id>
		</nuclide>
		<nuclide>
			<comp>0.99289</comp>
			<id>92238</id>
		</nuclide>
