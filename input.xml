			<comp>0.5</comp>
			<id>94239</id>
		</nuclide>
		<nuclide>
			<comp>0.5</comp>
			<id>95241</id>
		</nuclide>
	</recipe>
	<recipe>
		<basis>mass</basis>
		<name>r_mox</name>
		<nuclide>
			<comp>7</comp>
			<id>94239</id>
		</nuclide>
		<nuclide>
			<comp>93</comp>
			<id>92238</id>
		</nuclide>
	</recipe>
	<recipe>
		<basis>mass</basis>
		<name>r_mox_irrated</name>
		<nuclide>
			<comp>2</comp>
